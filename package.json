{
  "name": "middy-odata-v4",
  "version": "0.0.1",
  "type": "module",
  "description": "Middy middleware to handle OData v4.01 query options and responses for AWS Lambda",
  "license": "MIT",
  "author": "",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "dev": "vitest",
    "typecheck": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "publish:manual": "pnpm build && pnpm publish"
  },
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.8",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@middy/core": "^6.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lacoz/middy-odata-v4.git"
  },
  "keywords": [
    "middy",
    "middleware",
    "odata",
    "odata-v4",
    "aws-lambda",
    "api-gateway"
  ]
}
